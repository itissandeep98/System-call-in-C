+obj-y := sh_task_info.o
diff -ruN linux-3.17/sh_task_info/sh_task_info.c linux-3.17-original/sh_task_info/sh_task_info.c
--- linux-3.17/sh_task_info/sh_task_info.c	1970-01-01 05:30:00.000000000 +0530
+++ linux-3.17-original/sh_task_info/sh_task_info.c	2019-09-27 18:49:09.181234199 +0530
@@ -0,0 +1,69 @@
+#include <linux/module.h>
+#include <linux/sched.h>
+#include <linux/kernel.h>
+
+#include <linux/init.h>
+#include <linux/syscalls.h>
+#include <linux/fcntl.h>
+#include <asm/uaccess.h>
+#include <linux/fs.h>
+#include <linux/file.h>
+
+asmlinkage long sys_sh_task_info(int pid,char* name){
+	struct task_struct *task;
+	int flag=0;
+	struct file *file;
+	loff_t pos=0;
+	int fd;
+	char tempdata[100];
+	char data[200]="";
+	mm_segment_t old_fs=get_fs();
+	set_fs(KERNEL_DS);
+	if(pid<=0){
+		return 9;
+	}
+	for_each_process(task){
+		if(task->pid==pid){
+			printk("Process Name: %s\n",task->comm);
+			sprintf(tempdata,"Process Name: %s\n",task->comm);
+			strcat(data,tempdata);
+
+			printk("Pid: %ld\n",(long)task->pid);
+			sprintf(tempdata,"Pid: %ld\n",(long)task->pid);
+			strcat(data,tempdata);
+
+			printk("State: %li\n",(long)task->state);
+			sprintf(tempdata,"State: %li\n",(long)task->state);
+			strcat(data,tempdata);
+
+			printk("Priority: %ld\n",(long)task->prio);
+			sprintf(tempdata,"Priority: %ld\n",(long)task->prio);
+			strcat(data,tempdata);
+			if(task->parent){
+				printk("Parent Process: %s\n",task->parent->comm);
+				sprintf(tempdata,"Parent Process: %s\n",task->parent->comm);
+				strcat(data,tempdata);
+			}
+			fd=sys_open(name,O_WRONLY|O_CREAT,0644);
+			if(fd>=0){
+				sys_write(fd,data,strlen(data));
+				file=fget(fd);
+				if(file){
+					vfs_write(file,data,strlen(data),&pos);
+					fput(file);
+				}
+				sys_close(fd);
+			}
+			else{
+				return -EISDIR;
+			}
+			set_fs(old_fs);
+			flag=1;
+		}
+
+	}
+	if(flag==0){
+		return 9;
+	}
+	return 0;
+}
